{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "grafana" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "panels": [
    {
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value_and_name"
      },
      "targets": [
        {
          "expr": "mysql_up",
          "legendFormat": "MySQL Status",
          "refId": "A"
        },
        {
          "expr": "mysql_global_status_uptime_seconds",
          "legendFormat": "Uptime",
          "refId": "B"
        }
      ],
      "title": "MySQL Status e Uptime (s)",
      "transformations": [
        { "id": "seriesToColumns", "options": { "byField": "Time" } }
      ],
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value_and_name"
      },
      "targets": [
        {
          "expr": "mysql_global_status_threads_connected",
          "legendFormat": "Conexões Ativas",
          "refId": "A"
        },
        {
          "expr": "mysql_global_status_threads_running",
          "legendFormat": "Threads Executando",
          "refId": "B"
        }
      ],
      "title": "Conexões e Threads",
      "transformations": [
        { "id": "seriesToColumns", "options": { "byField": "Time" } }
      ],
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "red", "value": 1000 } ] },
          "unit": "qps"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "id": 3,
      "options": {
        "legend": { "calcs": ["max"], "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "sum(rate(mysql_global_status_commands_total[5m])) by (instance)",
          "legendFormat": "Queries/s",
          "refId": "A"
        },
        {
          "expr": "sum(rate(mysql_global_status_commands_total{command!~\"^(select|insert|update|delete|commit|rollback)$\"}[5m])) by (instance)",
          "legendFormat": "Outros Comandos/s",
          "refId": "B"
        }
      ],
      "title": "Taxa de Queries por Segundo (QPS)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "Bps"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "id": 4,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "rate(mysql_global_status_bytes_received_total[5m])",
          "legendFormat": "Bytes Recebidos",
          "refId": "A"
        },
        {
          "expr": "rate(mysql_global_status_bytes_sent_total[5m])",
          "legendFormat": "Bytes Enviados",
          "refId": "B"
        }
      ],
      "title": "Tráfego de Rede do MySQL",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "red", "value": 0.8 } ] },
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "id": 5,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "1 - (mysql_global_status_innodb_buffer_pool_pages_free / mysql_global_status_innodb_buffer_pool_pages_total)",
          "legendFormat": "Buffer Pool Uso",
          "refId": "A"
        }
      ],
      "title": "Uso do InnoDB Buffer Pool",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["mysql", "database", "prometheus"],
  "templating": { "list": [] },
  "time": { "from": "now-30m", "to": "now" },
  "timepicker": { "refresh_intervals": ["10s", "30s", "1m", "5m"] },
  "timezone": "",
  "title": "MySQL - Visão Geral",
  "uid": "mysql-observability",
  "version": 1
}